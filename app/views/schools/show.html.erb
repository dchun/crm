<%- model_class = School -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @school.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state_rank) %>:</strong></dt>
  <dd><%= @school.state_rank %></dd>
  <dt><strong><%= model_class.human_attribute_name(:national_rank) %>:</strong></dt>
  <dd><%= @school.national_rank %></dd>
  <dt><strong><%= model_class.human_attribute_name(:college_readiness_score) %>:</strong></dt>
  <dd><%= @school.college_readiness_score %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tested_ap_ib) %>:</strong></dt>
  <dd><%= @school.tested_ap_ib %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pass_ap_ib) %>:</strong></dt>
  <dd><%= @school.pass_ap_ib %></dd>
  <dt><strong><%= model_class.human_attribute_name(:math_score) %>:</strong></dt>
  <dd><%= @school.math_score %></dd>
  <dt><strong><%= model_class.human_attribute_name(:math_proficient) %>:</strong></dt>
  <dd><%= @school.math_proficient %></dd>
  <dt><strong><%= model_class.human_attribute_name(:math_not_proficient) %>:</strong></dt>
  <dd><%= @school.math_not_proficient %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reading_score) %>:</strong></dt>
  <dd><%= @school.reading_score %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reading_proficient) %>:</strong></dt>
  <dd><%= @school.reading_proficient %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reading_not_proficient) %>:</strong></dt>
  <dd><%= @school.reading_not_proficient %></dd>
  <dt><strong><%= model_class.human_attribute_name(:student_teacher_ratio) %>:</strong></dt>
  <dd><%= @school.student_teacher_ratio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:street_address) %>:</strong></dt>
  <dd><%= @school.street_address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:city) %>:</strong></dt>
  <dd><%= @school.city %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state) %>:</strong></dt>
  <dd><%= @school.state %></dd>
  <dt><strong><%= model_class.human_attribute_name(:zip) %>:</strong></dt>
  <dd><%= @school.zip %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @school.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @school.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:total_enrollment) %>:</strong></dt>
  <dd><%= @school.total_enrollment %></dd>
  <dt><strong><%= model_class.human_attribute_name(:total_economically_disadvantaged) %>:</strong></dt>
  <dd><%= @school.total_economically_disadvantaged %></dd>
  <dt><strong><%= model_class.human_attribute_name(:receives_title_i_funding) %>:</strong></dt>
  <dd><%= @school.receives_title_i_funding %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ap_student_performance_participation_rate) %>:</strong></dt>
  <dd><%= @school.ap_student_performance_participation_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ap_student_performance_participant_passing_rate) %>:</strong></dt>
  <dd><%= @school.ap_student_performance_participant_passing_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ap_student_performance_exam_per_test_taker) %>:</strong></dt>
  <dd><%= @school.ap_student_performance_exam_per_test_taker %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ap_student_performance_exam_pass_rate) %>:</strong></dt>
  <dd><%= @school.ap_student_performance_exam_pass_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:district_id) %>:</strong></dt>
  <dd><%= @school.district_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              schools_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_school_path(@school), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              school_path(@school),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
